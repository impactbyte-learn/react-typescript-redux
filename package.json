{
  "name": "react-parcel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development parcel public/index.html",
    "start": "NODE_ENV=production serve dist --single",
    "build": "NODE_ENV=production parcel build public/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf ./dist ./.cache",
    "precommit": "pretty-quick"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.css?$": "jest-css-modules-transform",
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp)$": "<rootDir>/src/__tests__/config/file-transformer.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/__tests__/config/setup.ts"
    ],
    "testRegex": "(/__tests__/*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [],
  "author": "Rahman Fadhil <rhmnfadhil@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-form": "^7.4.5",
    "@types/redux-logger": "^3.0.6",
    "@types/validator": "^9.4.1",
    "autoprefixer": "^9.1.3",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-css-modules-transform": "^2.0.0",
    "jest-enzyme": "^6.0.3",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-sw-precache": "^1.0.1",
    "postcss-modules": "^1.3.2",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.2",
    "serve": "^10.0.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "tailwindcss": "^0.6.5",
    "validator": "^10.7.1"
  }
}
